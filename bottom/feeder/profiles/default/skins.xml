<?xml version="1.0"?>
<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="bottom_feeder_custom_images"
    meta_type="Filesystem Directory View"
    directory="bottom.feeder:skins/bottom_feeder_custom_images"/>
 <object name="bottom_feeder_custom_templates"
    meta_type="Filesystem Directory View"
    directory="bottom.feeder:skins/bottom_feeder_custom_templates"/>
 <object name="bottom_feeder_styles"
    meta_type="Filesystem Directory View"
    directory="bottom.feeder:skins/bottom_feeder_styles"/>

 <skin-path name="Plone Default">
  <layer name="bottom_feeder_custom_images"
     insert-after="custom"/>
  <layer name="bottom_feeder_custom_templates"
     insert-after="bottom_feeder_custom_images"/>
  <layer name="bottom_feeder_styles"
     insert-after="bottom_feeder_custom_templates"/>
 </skin-path>

</object>
